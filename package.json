{
  "name": "nudged",
  "version": "1.0.0",
  "description": "Affine transformation estimator e.g. for multi-touch gestures and calibration",
  "keywords": [
    "transformation",
    "scale",
    "rotate",
    "translate",
    "pinch",
    "affine",
    "multitouch",
    "gesture",
    "estimator"
  ],
  "homepage": "https://github.com/axelpale/nudged",
  "main": "index.js",
  "author": {
    "name": "Akseli Palen",
    "email": "akseli.palen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/axelpale/nudged.git"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/axelpale/nudged/blob/master/LICENSE"
  },
  "dependencies": {},
  "devDependencies": {
    "jshint": "^2.8.0",
    "mocha": "^2.3.3",
    "should": "^7.1.0",
    "loadimages": "^0.2.2",
    "browserify": "^11.2.0",
    "hammerjs": "^2.0.4",
    "component-emitter": "^1.2.0",
    "lodash": "^3.10.1"
  },
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "mocha test/index.test.js",
    "test:lint": "jshint index.js lib/*.js test/*.js",
    "test:editor-example": "jshint editor-example/src/*.js",
    "test:touch-example": "jshint touch-example/src/*.js",
    "build:editor-example": "npm run test:editor-example && browserify editor-example/src/index.js -o editor-example/app.js",
    "build:touch-example": "npm run test:touch-example && browserify touch-example/src/index.js -o touch-example/app.js",
    "build:examples": "npm run build:touch-example && npm run build:editor-example",
    "build:standalone": "browserify index.js --standalone nudged -o nudged.js"
  }
}
